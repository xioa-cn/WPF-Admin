<UserControl x:Class="Xioa.Admin.Core.Views.LiveChartsTemplate.Component.SlimGaugeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
             xmlns:vm="clr-namespace:Xioa.Admin.Core.Views.LiveChartsTemplate.ViewModel"
             d:DataContext="{d:DesignInstance Type=vm:SlimGaugeViewModel}"
             xmlns:local="clr-namespace:Xioa.Admin.Core.Views.LiveChartsTemplate.Component"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <lvc:PieChart
        Series="{Binding Series}"
        InitialRotation="-90"
        MaxAngle="350"
        MinValue="0"
        MaxValue="100">
    </lvc:PieChart>
</UserControl>
