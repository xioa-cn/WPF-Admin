<UserControl x:Class="DataAcquisition.Core.Views.LiveChartsTemplate.Component.RadialGradientsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
             xmlns:vm="clr-namespace:DataAcquisition.Core.Views.LiveChartsTemplate.ViewModel"
             d:DataContext="{d:DesignInstance Type=vm:RadialGradientsViewModel}"
             xmlns:local="clr-namespace:DataAcquisition.Core.Views.LiveChartsTemplate.Component"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <lvc:PieChart Series="{Binding Series}" LegendPosition="Right"></lvc:PieChart>
</UserControl>
