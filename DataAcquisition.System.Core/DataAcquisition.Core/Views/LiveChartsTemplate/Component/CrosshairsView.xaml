<UserControl x:Class="DataAcquisition.Core.Views.LiveChartsTemplate.Component.CrosshairsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
             xmlns:vm="clr-namespace:DataAcquisition.Core.Views.LiveChartsTemplate.ViewModel"
             d:DataContext="{d:DesignInstance Type=vm:CrosshairsViewModel}"
             xmlns:local="clr-namespace:DataAcquisition.Core.Views.LiveChartsTemplate.Component"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <lvc:CartesianChart
        Series="{Binding Series}"
        XAxes="{Binding XAxes}"
        YAxes="{Binding YAxes}">
    </lvc:CartesianChart>
</UserControl>
